    <CoreObject name="Text_Paragraph" notExtendable="false">
        <Documentation>
            <Description>An indication of a new paragraph for a sub-section of a formatted text message.</Description>
        </Documentation>
        <Aliases>Text_Comments Description</Aliases>
        <Simple type="ota2:String_Text">
            <Documentation>
                <Description>Simple text content.</Description>
            </Documentation>
        </Simple>
        <Summary>
            <Attribute name="name" type="ota2:String_Tiny">
                <Documentation>
                    <Description>This will allow you to define the information that is being sent, typically used when multiple occurrences of ParagraphType are being sent.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</Implementer>
                </Documentation>
                <Equivalent context="Common">Name</Equivalent>
                <Example context="Common">paragraph name</Example>
            </Attribute>
            <Attribute name="number" type="xsd:positiveInteger">
                <Documentation>
                    <Description>The sequence number for the paragraph.</Description>
                    <Implementer>Changed type from {http://www.w3.org/2001/XMLSchema}nonNegativeInteger.</Implementer>
                    <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
                </Documentation>
                <Equivalent context="Common">ParagraphNumber</Equivalent>
            </Attribute>
            <Attribute name="language" type="xsd:language">
                <Documentation>
                    <Description>The language for this paragraph.</Description>
                    <Implementer>AttributeGroup: LanguageGroup</Implementer>
                </Documentation>
                <Example context="Common">en</Example>
