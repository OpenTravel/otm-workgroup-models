    <CoreObject name="Person_Name" notExtendable="false">
        <Documentation>
            <Description>This provides name information for a person.</Description>
        </Documentation>
        <Aliases>RelatedTraveler_Simple_Person_Name</Aliases>
        <Simple type="ota2:String">
            <Documentation>
                <Description>Full name.</Description>
            </Documentation>
        </Simple>
        <Summary>
            <Attribute name="type" type="NameType">
                <Documentation>
                    <Description>Type of name of the individual, such as former, nickname, alternate or alias name. Refer to OpenTravel Code List Name Type (NAM).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</Implementer>
                </Documentation>
                <Equivalent context="Common">NameType</Equivalent>
            </Attribute>
            <Attribute name="language" type="xsd:language">
                <Documentation>
                    <Description>The language the name is represented in, e.g. an Eastern name versus a Western name.</Description>
                    <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}language</Implementer>
                </Documentation>
                <Equivalent context="Common">Language</Equivalent>
            </Attribute>
            <Element name="Prefix" repeat="3" type="ota2:String_Tiny">
                <Documentation>
                    <Description>Salutation of honorific (e.g. Mr., Mrs., Ms., Miss, Dr.)</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</Implementer>
                </Documentation>
                <Equivalent context="Common">NamePrefix</Equivalent>
                <Example context="Common">Mr.</Example>
            </Element>
            <Element name="Given" repeat="5" type="ota2:String_Short">
                <Documentation>
                    <Description>Given name, first name or names.</Description>
                </Documentation>
                <Example context="Common">John</Example>
            </Element>
            <Element name="Middle" repeat="5" type="ota2:String_Short">
                <Documentation>
                    <Description>The middle name of the person name.</Description>
                </Documentation>
            </Element>
            <Element name="SurnamePrefix" repeat="1" type="ota2:String_Tiny">
                <Documentation>
                    <Description>The surname prefix, e.g "van der", "von", "de".</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</Implementer>
                </Documentation>
                <Equivalent context="Common">SurnamePrefix</Equivalent>
            </Element>
            <Element mandatory="true" name="Surname" repeat="0" type="ota2:String_Short">
                <Documentation>
                    <Description>Family name, last name. 
Note: Simple form of person name should be used for the full name.
May also be used for full name if the sending system does not have the ability to separate a full name into its parts, e.g. the surname element may be used to pass the full name.</Description>
                </Documentation>
                <Example context="Common">Smith</Example>
            </Element>
            <Element name="Suffix" repeat="3" type="ota2:String_Tiny">
                <Documentation>
                    <Description>Hold various name suffixes and letters (e.g. Jr., Sr., III, Ret., Esq.)</Description>
                </Documentation>
            </Element>
            <Element name="Title" repeat="3" type="ota2:String_Tiny">
                <Documentation>
                    <Description>Degree or honors (e.g., Ph.D., M.D.)</Description>
                </Documentation>
            </Element>
            <Element name="Person_Document" repeat="1" type="Person_Document">
                <Documentation>
                    <Description>Document  information for verification purposes and also used for additional filtering for common names.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}Document</Implementer>
                </Documentation>
                <Equivalent context="Common">Document</Equivalent>
            </Element>
            <Element name="Privacy" repeat="0" type="Privacy"/>
            <Indicator name="defaultInd" publishAsElement="false">
                <Documentation>
                    <Description>If true, this is the default or primary name within a collection of names.</Description>
                </Documentation>
            </Indicator>
        </Summary>
        <Detail/>
    </CoreObject>
    
