    <CoreObject name="ClubMembership" notExtendable="false">
        <Documentation>
            <Description>Identifies a travel club that offers special privileges and related services.</Description>
        </Documentation>
        <Aliases/>
        <Simple type="ota2:Empty"/>
        <Summary>
            <Element mandatory="true" name="Name" repeat="1" type="CompanyName">
                <Documentation>
                    <Description>Name of the travel club.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}CompanyNameType</Implementer>
                </Documentation>
                <Equivalent context="Common">TravelClubName</Equivalent>
            </Element>
            <Element name="MemberIdentifier" repeat="1" type="ota2:String_Short">
                <Documentation>
                    <Description>Identifier assigned to person registered with travel club.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}ClubMemberName</Implementer>
                </Documentation>
                <Equivalent context="Common">ClubMemberName</Equivalent>
            </Element>
            <Element mandatory="true" name="Date_EffectiveExpire"
                repeat="0" type="Date_EffectiveExpire">
                <Documentation>
                    <Description>Used to identify the effective date and/or expiration date.</Description>
                </Documentation>
            </Element>
            <Element mandatory="true" name="Privacy" repeat="0" type="Privacy"/>
        </Summary>
        <Detail/>
    </CoreObject>
    
