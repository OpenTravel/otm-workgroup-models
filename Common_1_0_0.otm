    <CoreObject name="Address" notExtendable="false">
        <Documentation>
            <Description>Provides address information. Unformatted addresses are captured using the simple facet.</Description>
        </Documentation>
        <Aliases/>
        <Simple type="ota2:String_Long">
            <Documentation>
                <Description>When the address is unformatted (FormattedInd="false") these lines will contain free form address details. When the address is formatted and street number and street name must be sent independently, the street number will be sent using StreetNmbr, and the street name will be sent in the first AddressLine occurrence.</Description>
                <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to255</Implementer>
            </Documentation>
        </Simple>
        <Summary>
            <Element name="AddressLine" repeat="4" type="ota2:String_Short">
                <Documentation>
                    <Description>Address in an unstructured format.</Description>
                </Documentation>
            </Element>
            <Element name="BldgRoom" repeat="2" type="Address_BldgRoom">
                <Documentation>
                    <Description>Building name, room, apartment, or suite number.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}BldgRoom</Implementer>
                </Documentation>
                <Equivalent context="Common">BldgRoom</Equivalent>
            </Element>
            <Element name="Number" repeat="1" type="Address_StreetNumber">
                <Documentation>
                    <Description>May contain the street number and optionally the street name.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StreetNmbr</Implementer>
                </Documentation>
                <Equivalent context="Common">StreetNmbr</Equivalent>
                <Example context="Common">111</Example>
            </Element>
            <Element name="Street" repeat="1" type="ota2:String_Short">
                <Documentation>
                    <Description>May contain the street number when the Street number element is missing.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to255</Implementer>
                </Documentation>
                <Equivalent context="Common">AddressLine</Equivalent>
                <Example context="Common">Travers Street</Example>
            </Element>
            <Element name="City" repeat="1" type="ota2:String_Short">
                <Documentation>
                    <Description>City (e.g., Dublin), town, or postal station (i.e., a postal service territory, often used in a military address).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</Implementer>
                </Documentation>
                <Equivalent context="Common">CityName</Equivalent>
                <Example context="Common">Dublin</Example>
            </Element>
            <Element name="County" repeat="1" type="ota2:String_Short">
                <Documentation>
                    <Description>County or Region Name (e.g., Fairfax).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</Implementer>
                </Documentation>
                <Equivalent context="Common">County</Equivalent>
                <Example context="Common">Fairfax</Example>
            </Element>
            <Element name="StateProv" repeat="1" type="StateProv">
                <Documentation>
                    <Description>State or Province name (e.g., Texas).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StateProvType</Implementer>
                </Documentation>
                <Equivalent context="Common">StateProv</Equivalent>
                <Example context="Common">Texas</Example>
            </Element>
            <Element name="Country" repeat="1" type="Country">
                <Documentation>
                    <Description>Country name (e.g., Ireland).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}CountryNameType</Implementer>
                </Documentation>
                <Equivalent context="Common">CountryName</Equivalent>
                <Example context="Common">IE</Example>
            </Element>
            <Element name="PostalCode" repeat="1" type="ota2:String_Short">
                <Documentation>
                    <Description>Post Office Code number.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</Implementer>
                </Documentation>
                <Equivalent context="Common">PostalCode</Equivalent>
                <Example context="Common">32165-9999</Example>
            </Element>
        </Summary>
        <Detail>
            <Attribute name="Language" type="xsd:language">
                <Documentation>
                    <Description>Example: en</Description>
                    <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}language</Implementer>
                </Documentation>
                <Equivalent context="Common">Language</Equivalent>
                <Example context="Common">en</Example>
            </Attribute>
            <Attribute name="type" type="Code_OTA">
                <Documentation>
                    <Description>Defines the type of address (e.g. home, business, other). Refer to OpenTravel Code List Communication Location Type (CLT).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</Implementer>
                </Documentation>
                <Equivalent context="Common">Type</Equivalent>
                <Example context="Common">CLT</Example>
            </Attribute>
            <Attribute name="use" type="Code_OTA">
                <Documentation>
                    <Description>Describes the use of the address (e.g. mailing, delivery, billing, etc.). Refer to OpenTravel Code List Address Use Type (AUT).</Description>
                </Documentation>
                <Example context="Common">AUT</Example>
            </Attribute>
            <Element name="Remark" repeat="0" type="ota2:String_Text">
                <Documentation>
                    <Description>A remark associated with this address.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</Implementer>
                </Documentation>
                <Equivalent context="Common">Remark</Equivalent>
                <Example context="Common">This address should be used for all official correspondences.</Example>
            </Element>
            <Element name="Privacy" repeat="0" type="Privacy">
                <Documentation>
                    <Description>Allows for control of the sharing of address information between parties.</Description>
                    <Implementer>AttributeGroup: PrivacyGroup</Implementer>
                </Documentation>
            </Element>
            <Indicator name="validInd" publishAsElement="false">
                <Documentation>
                    <Description>If true, this is a valid and complete mailing address that has been verified through an address verification service or previously mailed materials have not been returned.</Description>
                </Documentation>
            </Indicator>
        </Detail>
        <Roles>
            <Role value="Home"/>
            <Role value="Business"/>
            <Role value="Destination"/>
            <Role value="Other"/>
        </Roles>
    </CoreObject>
    
