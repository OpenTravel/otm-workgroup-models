        <Value label="" literal="Exclusive"/>
        <Value label="" literal="Cumulative"/>
    </Enumeration_Closed>
    
    <CoreObject name="Tax" notExtendable="false">
        <Documentation>
            <Description>An applicable tax. It allows for a percentage or a flat amount. If one field is used, the other should not be used since logically taxes should be calculated in only one of the two ways.</Description>
        </Documentation>
        <Aliases/>
        <Simple type="ota2:Empty"/>
        <Summary>
            <Attribute name="sequenceNbr" type="ota2-0300:String">
                <Documentation>
                    <Description>A number assigned by the responding system to designate the order of this tax among multiple taxes.</Description>
                    <Implementer>Changed type from {http://www.w3.org/2001/XMLSchema}nonNegativeInteger.</Implementer>
                    <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
                </Documentation>
            </Attribute>
            <Element name="Amount" repeat="0" type="ota2-0300:Amount">
                <Documentation>
                    <Description>The breakdown of the tax including either the amount or percent of the tax.</Description>
                </Documentation>
            </Element>
            <Element name="EffectiveExpire" repeat="0" type="ota2-0300:DateEffectiveExpire">
                <Documentation>
                    <Description>Provides the effective and expiry dates for the tax.</Description>
                    <Implementer>AttributeGroup: EffectiveExpireOptionalDateGroup</Implementer>
                </Documentation>
            </Element>
            <Element name="TaxType_Enum" repeat="0" type="ota2-0200:TaxType_Enum">
                <Documentation>
                    <Description>Identifies the tax (eg. Sales Tax, State Tax)</Description>
                </Documentation>
                <Example context="Order"/>
            </Element>
        </Summary>
        <Detail/>
    </CoreObject>
    
    <CoreObject name="Taxes" notExtendable="false">
        <Documentation>
            <Description>A collection of taxes.</Description>
        </Documentation>
        <Aliases/>
        <Simple type="ota2:Empty"/>
        <Summary>
            <Element mandatory="true" name="Total" repeat="0" type="ota2-0300:CurrencyAmount">
                <Documentation>
                    <Description>Used to provide a total of all the taxes.</Description>
                    <Implementer>AttributeGroup: CurrencyAmountGroup</Implementer>
                </Documentation>
            </Element>
            <Element name="Tax" repeat="99" type="Tax">
                <Documentation>
                    <Description>An individual tax.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}TaxType</Implementer>
                </Documentation>
            </Element>
        </Summary>
        <Detail/>
    </CoreObject>
    
    <CoreObject name="RebateProgram" notExtendable="false">
        <Documentation>
            <Description>Information about a suppliers participation in a rebate program, e.g. a VAT (value added tax) program.</Description>
        </Documentation>
        <Aliases/>
        <Simple type="ota2:Empty"/>
        <Summary>
            <Attribute name="programName" type="ota2-0300:String">
                <Documentation>
                    <Description>The name of the rebate program.</Description>
                    <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
                </Documentation>
            </Attribute>
            <Element name="PaymentCard" repeat="9" type="fin-0200:PaymentCard">
                <Documentation>
                    <Description>Information about payments eligible for a rebate program.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}PaymentCardType</Implementer>
                </Documentation>
            </Element>
            <Element name="ProgramRestrictions" repeat="0" type="ota2-0300:TextFree">
                <Documentation>
                    <Description>Describes restrictions related to the rebate program.</Description>
                </Documentation>
            </Element>
            <Indicator name="participationInd" publishAsElement="false">
                <Documentation>
                    <Description>When true, the supplier is participating in a rebate program, e.g. a VAT (value added tax) program.</Description>
                </Documentation>
            </Indicator>
        </Summary>
        <Detail/>
    </CoreObject>
    
    <CoreObject name="Fee" notExtendable="false">
        <Aliases/>
        <Simple type="ota2:Empty"/>
