    <Context applicationContext="http://www.opentravel.org/OTM/order/v0" context="Order"/>
    
    <BusinessObject name="Offer" notExtendable="false">
        <Documentation>
            <Description>A product or products available at the given price and terms.</Description>
        </Documentation>
        <Aliases/>
        <ID>
            <Attribute name="objID" type="xsd:ID">
                <Documentation>
                    <Description>A unique identifier within this document for this object.</Description>
                </Documentation>
            </Attribute>
            <Element name="Identifier" repeat="0" type="ota2-0300:Identifier">
                <Documentation>
                    <Description>A unique identifier defined by an external authority for this object.</Description>
                </Documentation>
            </Element>
            <Element name="OfferRef" reference="true" repeat="0" type="Offer">
                <Documentation>
                    <Description>Refers to the objID on an object defined elsewhere in the current document.</Description>
                </Documentation>
            </Element>
        </ID>
        <Summary>
            <Attribute name="vendor" type="ota2-0300:StringTiny">
                <Documentation>
                    <Description>Specifies the vendor for this offer.</Description>
                </Documentation>
                <Example context="Order">Vendor Name</Example>
            </Attribute>
            <Element mandatory="true" name="TotalPrice" repeat="0" type="TotalPrice">
                <Documentation>
                    <Description>Total price for all the products. Price may include itemized price information</Description>
                </Documentation>
            </Element>
            <Element name="TermsAndConditions" repeat="25" type="TermsAndConditions">
                <Documentation>
                    <Description>Specifies the terms and conditions for this offer.</Description>
                </Documentation>
            </Element>
            <Element name="Product" repeat="30" type="prod-0300:Product">
                <Documentation>
                    <Description>One or more products that make up this offer.</Description>
                </Documentation>
            </Element>
            <Element name="GuaranteesAccepted" repeat="1" type="GuaranteesAccepted">
                <Documentation>
                    <Description>A list of guarantees accepted</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}GuaranteesAccepted</Implementer>
                </Documentation>
            </Element>
        </Summary>
        <Detail/>
        <Query context="Order" label="Offer" notExtendable="false">
            <Element mandatory="true" name="ProductQueries" repeat="2" type="prod-0300:ProductQueries">
                <Documentation>
                    <Description>Use 2 if requesting round trip Offers</Description>
                </Documentation>
            </Element>
        </Query>
        <Custom context="Order" label="Confirmed" notExtendable="false">
            <Attribute name="description" type="ota2-0300:StringLong">
                <Documentation>
                    <Description>A description of the confirmed offer.</Description>
                </Documentation>
            </Attribute>
            <Element name="ConfirmationId" repeat="0" type="ota2-0300:Identifier">
                <Documentation>
                    <Description>An identifier for the confirmed offer provided by the vendor.</Description>
                </Documentation>
            </Element>
        </Custom>
    </BusinessObject>
    
    <BusinessObject name="Order" notExtendable="false">
        <Aliases/>
        <ID>
            <Attribute name="objID" type="xsd:ID">
                <Documentation>
                    <Description>A unique identifier within this document for this object.</Description>
                </Documentation>
            </Attribute>
            <Element name="Identifier" repeat="0" type="ota2-0300:Identifier">
                <Documentation>
                    <Description>A unique identifier defined by an external authority for this object.  This is the confirmation number for the order as a whole and may include a version for the order.  When placing an order, this should be left blank.</Description>
                </Documentation>
            </Element>
            <Element name="OrderRef" reference="true" repeat="0" type="Order">
                <Documentation>
                    <Description>Refers to the objID on an object defined elsewhere in the current document.</Description>
                </Documentation>
            </Element>
        </ID>
        <Summary>
            <Element mandatory="true" name="Offer" repeat="99" type="Offer">
                <Documentation>
                    <Description>Offers will be used to place an order and will typically be replaced with confirmed offer facets when the vendor(s) confirm that they have been accepted.</Description>
                </Documentation>
            </Element>
            <Element mandatory="true" name="Traveler" repeat="1" type="ota2-0300:Traveler">
                <Documentation>
                    <Description>The primary traveler for this order.</Description>
                </Documentation>
            </Element>
            <Element name="RelatedTraveler" repeat="98" type="ota2-0300:RelatedTraveler">
                <Documentation>
                    <Description>Additional traveler(s) for this order.</Description>
                </Documentation>
            </Element>
            <Element name="Payment" repeat="5" type="Payment">
                <Documentation>
                    <Description>Payment details for this order.</Description>
                </Documentation>
            </Element>
            <Element name="TotalPrice" repeat="0" type="TotalPrice">
                <Documentation>
                    <Description>Total price for the order.</Description>
                </Documentation>
            </Element>
            <Element name="AcceptableGuarantee" repeat="0" type="AcceptableGuarantee">
                <Documentation>
                    <Description>Guarantee payment information (if required) for an order.</Description>
                </Documentation>
            </Element>
            <Element name="Commission" repeat="9" type="Commission">
                <Documentation>
                    <Description>Commission information associated with the order</Description>
                </Documentation>
            </Element>
            <Element name="PersonProductRef" repeat="99" type="PersonProductRef">
                <Documentation>
                    <Description>Provides a reference between a person and a product.</Description>
                </Documentation>
            </Element>
            <Element name="TripPurpose" repeat="0" type="ota2-0300:Code">
                <Documentation>
                    <Description>Specifies the purpose of the trip.</Description>
                </Documentation>
            </Element>
        </Summary>
        <Detail/>
        <Custom context="Order" label="Confirmation" notExtendable="false">
            <Attribute name="description" type="ota2-0300:StringLong">
                <Documentation>
                    <Description>A description for a confirmation.</Description>
                </Documentation>
            </Attribute>
            <Element name="ConfirmationId" repeat="0" type="ota2-0300:Identifier">
                <Documentation>
                    <Description>An identifier that is the confirmation of this order being placed by the organization that accepted this order.</Description>
                </Documentation>
            </Element>
        </Custom>
    </BusinessObject>
    
    <BusinessObject name="TermsAndConditions" notExtendable="false">
        <Documentation>
            <Description>The terms and conditions related to an offer.</Description>
        </Documentation>
        <Aliases/>
        <ID>
            <Attribute name="objID" type="xsd:ID">
                <Documentation>
                    <Description>A unique identifier within this document for this object.</Description>
                </Documentation>
            </Attribute>
            <Element name="Identifier" repeat="0" type="ota2-0300:Identifier">
                <Documentation>
                    <Description>A unique identifier defined by an external authority for this object.</Description>
                </Documentation>
            </Element>
            <Element name="TermsAndConditionsRef" reference="true"
                repeat="0" type="TermsAndConditions">
                <Documentation>
                    <Description>Refers to the objID on an object defined elsewhere in the current document.</Description>
                </Documentation>
            </Element>
        </ID>
        <Summary>
            <Attribute name="duration" type="xsd:positiveInteger">
                <Documentation>
                    <Description>Specifies the duration of the trip.</Description>
                </Documentation>
            </Attribute>
            <Element name="Promotion" repeat="0" type="ota2-0300:Promotion">
                <Documentation>
                    <Description>Specifies the terms and conditions as related to the promotion.</Description>
                </Documentation>
            </Element>
            <Element name="RebateProgram" repeat="25" type="RebateProgram">
                <Documentation>
                    <Description>Specifies the details of the rebate program.</Description>
                </Documentation>
            </Element>
            <Element name="Description" repeat="0" type="ota2-0300:Description">
                <Documentation>
                    <Description>Details of the terms and conditions.</Description>
                </Documentation>
            </Element>
            <Element name="CancelPenalty" repeat="10" type="CancelPenalty">
                <Documentation>
                    <Description>Specifies the cancellation penalty.</Description>
                </Documentation>
            </Element>
            <Element name="AcceptableGuarantee" repeat="0" type="AcceptableGuarantee">
                <Documentation>
                    <Description>Guarantee information.</Description>
                </Documentation>
            </Element>
        </Summary>
        <Detail/>
    </BusinessObject>
    
    <BusinessObject name="Payment" notExtendable="false">
        <Documentation>
            <Description>Payment information which may be related to one or more offers.</Description>
        </Documentation>
        <Aliases/>
        <ID>
            <Attribute name="objID" type="xsd:ID">
                <Documentation>
                    <Description>A unique identifier within this document for this object.</Description>
                </Documentation>
            </Attribute>
            <Element name="Identifier" repeat="0" type="ota2-0300:Identifier">
                <Documentation>
                    <Description>A unique identifier defined by an external authority for this object.</Description>
                </Documentation>
            </Element>
            <Element name="PaymentRef" reference="true" repeat="0" type="Payment">
                <Documentation>
                    <Description>Refers to the objID on an object defined elsewhere in the current document.</Description>
                </Documentation>
            </Element>
        </ID>
        <Summary>
            <Element mandatory="true" name="FormOfPayment" repeat="5" type="fin-0200:FormOfPayment">
                <Documentation>
                    <Description>Specifies the form of payment.</Description>
                </Documentation>
            </Element>
            <Element mandatory="true" name="Amount" repeat="0" type="ota2-0300:CurrencyAmount">
                <Documentation>
                    <Description>Total payment amount</Description>
                </Documentation>
            </Element>
            <Element name="Person" repeat="99" type="ota2-0300:Person">
                <Documentation>
                    <Description>The person making the payment.</Description>
                </Documentation>
            </Element>
            <Element name="OfferRef" reference="true" repeat="99" type="Offer">
                <Documentation>
                    <Description>The offer that the payment is for.</Description>
                </Documentation>
            </Element>
        </Summary>
        <Detail/>
    </BusinessObject>
    
    <CoreObject name="TotalPrice" notExtendable="false">
        <Documentation>
            <Description>The total price of the offer incorporating fees and taxes for each product in the offer adjusted according to the codes in the price modifiers.</Description>
        </Documentation>
        <Aliases/>
        <Simple type="ota2-0300:Money">
            <Documentation>
                <Description>The total price for the offer or order.</Description>
            </Documentation>
        </Simple>
        <Summary>
            <Element mandatory="true" name="Total" repeat="0" type="ota2-0300:CurrencyAmount">
                <Documentation>
                    <Description>The total price for the offer or order.</Description>
                </Documentation>
            </Element>
            <Element name="ProductPrice" repeat="99" type="ProductPrice">
                <Documentation>
                    <Description>Price for a given Product reference.</Description>
                </Documentation>
            </Element>
            <Element name="PriceModifiers" repeat="99" type="PriceModifiers">
                <Documentation>
                    <Description>Promotions or discounts applied in the price.</Description>
                </Documentation>
            </Element>
            <Element name="Fee" repeat="99" type="Fee">
                <Documentation>
                    <Description>One or more fees associated with the offer or order.</Description>
                </Documentation>
            </Element>
            <Element name="Taxes" repeat="0" type="Taxes">
                <Documentation>
                    <Description>Taxes associated with the offer or order.</Description>
                </Documentation>
            </Element>
            <Indicator name="estimateInd" publishAsElement="false">
                <Documentation>
                    <Description>When true, indicates the total price is an estimate.</Description>
                </Documentation>
            </Indicator>
            <Indicator name="taxExcludedInd" publishAsElement="false">
                <Documentation>
                    <Description>When true, tax is excluded from the total.</Description>
                </Documentation>
            </Indicator>
        </Summary>
        <Detail/>
    </CoreObject>
    
    <CoreObject name="AcceptableGuarantee" notExtendable="false">
        <Documentation>
            <Description>The guarantee information to hold a reservation</Description>
        </Documentation>
        <Aliases/>
        <Simple type="ota2:Empty"/>
        <Summary>
            <Attribute name="retribution" type="Retribution_Enum">
                <Documentation>
                    <Description>An enumerated type defining the type of action taken when the deadline has been exceeded. Valid values: Res Automatically Cancelled, Res No Longer Guaranteed.</Description>
                    <Implementer>LocalAnonymousType: GuaranteeType_RetributionType</Implementer>
                </Documentation>
            </Attribute>
            <Attribute name="code" type="ota2-0300:StringTiny">
                <Documentation>
                    <Description>Guarantee Code</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</Implementer>
                </Documentation>
            </Attribute>
            <Attribute name="type" type="GuaranteeType_Enum">
                <Documentation>
                    <Description>An enumerated type defining the guarantee to be applied to this reservation.</Description>
                    <Implementer>LocalAnonymousType: GuaranteeType_GuaranteeType</Implementer>
                </Documentation>
            </Attribute>
            <Attribute name="holdTime" type="xsd:time">
                <Documentation>
                    <Description>The room will held up until this time without a guarantee.</Description>
                    <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}time</Implementer>
                </Documentation>
            </Attribute>
            <Attribute name="guaranteePolicyType" type="GuaranteeType_Enum">
                <Documentation>
                    <Description>GuaranteePolicyType: An enumerated type that defines the guarantee policy applied to the booking restrictions.</Description>
                </Documentation>
            </Attribute>
            <Element name="Deadline" repeat="1" type="Deadline">
                <Documentation>
                    <Description>The absolute deadline or amount of offset time before a deadline for a payment of cancel goes into effect.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}Deadline</Implementer>
                </Documentation>
            </Element>
            <Element name="Comment" repeat="20" type="ota2-0300:Comment">
                <Documentation>
                    <Description>List of comments.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}CommentType</Implementer>
                </Documentation>
            </Element>
            <Element name="Description" repeat="9" type="ota2-0300:Description">
                <Documentation>
                    <Description>Text description of the Guarantee in a given language.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}ParagraphType</Implementer>
                </Documentation>
            </Element>
            <Element name="CurrencyCode" repeat="0" type="ota2-0300:CurrencyCode">
                <Documentation>
                    <Description>An ISO 4217 (3) alpha character code that specifies a monetary unit.</Description>
                </Documentation>
            </Element>
            <Element name="PaymentType" repeat="0" type="ota2-0200:PaymentType_Enum">
                <Documentation>
                    <Description>Refer to OpenTravel Code List Payment Type (PMT).</Description>
                </Documentation>
            </Element>
        </Summary>
        <Detail/>
    </CoreObject>
    
    <CoreObject name="PersonProductRef" notExtendable="false">
