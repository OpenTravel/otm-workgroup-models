    <CoreObject name="CancelPenalty" notExtendable="false">
        <Documentation>
            <Description>The CancelPenalty class defines the cancellation policy of the hotel facility.</Description>
        </Documentation>
        <Aliases/>
        <Simple type="ota2:Empty"/>
        <Summary>
            <Element name="Deadline" repeat="1" type="Deadline">
                <Documentation>
                    <Description>Cancellation deadline, absolute or relative.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}Deadline</Implementer>
                </Documentation>
            </Element>
            <Element name="AmountPercent" repeat="0" type="ota2-0300:AmountPercent">
                <Documentation>
                    <Description>The amount or percent of the cancel penalty.</Description>
                </Documentation>
            </Element>
            <Element name="Description" repeat="9" type="ota2-0300:Description">
                <Documentation>
                    <Description>Text description of the Penalty in a given language.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}ParagraphType</Implementer>
                </Documentation>
            </Element>
        </Summary>
        <Detail>
            <Attribute name="policyId" type="ota2-0300:StringTiny">
                <Documentation>
                    <Description>An idenfier for the policy.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</Implementer>
                </Documentation>
            </Attribute>
            <Indicator name="nonRefundableInd" publishAsElement="false">
                <Documentation>
                    <Description>Indicates that any prepayment for the reservation is non refundable, therefore a 100% penalty on the prepayment is applied, irrespective of deadline.</Description>
                </Documentation>
            </Indicator>
            <Indicator name="noCancelInd" publishAsElement="false">
                <Documentation>
                    <Description>Note: This will typically be set to false indicating that a reservation may be cancelled.</Description>
                </Documentation>
            </Indicator>
        </Detail>
