    <CoreObject name="CommissionableAmount" notExtendable="false">
        <Aliases/>
        <Simple type="ota2:Empty"/>
        <Summary>
            <Element name="CurrencyAmountGroup" repeat="0" type="ota2-0300:CurrencyAmount">
                <Documentation>
                    <Description>Provides the currency amount on which the commission is applied.</Description>
                    <Implementer>AttributeGroup: CurrencyAmountGroup</Implementer>
                </Documentation>
            </Element>
            <Indicator name="taxInclusiveInd" publishAsElement="false">
                <Documentation>
                    <Description>When true, indicates that the commission is calculated using the rate including tax. When false, indicates that the commission is calculated using the net rate.</Description>
                </Documentation>
            </Indicator>
        </Summary>
        <Detail/>
    </CoreObject>
    
    <Enumeration_Closed name="Commission_Enum">
