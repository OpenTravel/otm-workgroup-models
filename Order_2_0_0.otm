                <Documentation>
                    <Description>When true, tax is excluded from the price.</Description>
                </Documentation>
            </Indicator>
        </Summary>
        <Detail/>
    </CoreObject>
    
    <CoreObject name="ProductPrice" notExtendable="false">
        <Aliases/>
        <Simple type="ota2:Empty"/>
