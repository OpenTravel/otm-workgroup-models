    <ChoiceObject name="Deadline" notExtendable="false">
        <Documentation>
            <Description>Allows a choice between a time period range, an absolute date or a date range for the appication of a cancellation penalty.</Description>
        </Documentation>
        <Aliases/>
        <Shared>
            <Attribute name="deadlineType" type="pgt-0100:DeadlineDropTime_Enum">
