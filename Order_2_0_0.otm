                <Documentation>
                    <Description>The minimum time period.</Description>
                </Documentation>
            </Attribute>
            <Attribute name="maxPeriod" type="xsd:integer">
                <Documentation>
                    <Description>The maximum time period</Description>
                </Documentation>
            </Attribute>
            <Attribute name="timePeriod" type="ota2-0300:StringTiny">
