            <Element name="PersonIdentifier" repeat="0" type="ota2-0300:Person_ID">
                <Documentation>
                    <Description>A person reference.</Description>
                </Documentation>
            </Element>
        </Summary>
        <Detail/>
    </CoreObject>
    
    <CoreObject name="PersonDiscount" notExtendable="false">
