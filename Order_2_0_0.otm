    <ValueWithAttrs name="Policy" type="ota2-0300:StringLong">
        <Documentation>
            <Description>Policy information regarding the ancillary for example Power Cart rental policy.</Description>
        </Documentation>
        <Attribute name="type" type="ota2-0300:StringShort">
            <Documentation>
                <Description>The type of the policy, such as "Tee Time" or "Cart Rental".</Description>
            </Documentation>
        </Attribute>
    </ValueWithAttrs>
    
    <Enumeration_Closed name="Applicability_Enum">
