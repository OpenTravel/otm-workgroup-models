    <ns2:CoreObject name="StayDay" notExtendable="false">
        <ns2:Documentation>
            <ns2:Description>Sleeping room details for a specific day of the event.</ns2:Description>
        </ns2:Documentation>
        <ns2:Aliases/>
        <ns2:Simple type="ota2:Empty"/>
        <ns2:Summary>
            <ns2:Attribute mandatory="true" name="dayNumber"
