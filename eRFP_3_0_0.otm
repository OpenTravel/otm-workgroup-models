                </ns2:Documentation>
            </ns2:Element>
        </ns2:Summary>
        <ns2:Detail/>
    </ns2:CoreObject>
    
    <ns2:CoreObject name="StayDayRoom" notExtendable="false">
        <ns2:Documentation>
            <ns2:Description>Specifies the room requirements for this day of the room block.</ns2:Description>
        </ns2:Documentation>
        <ns2:Aliases/>
        <ns2:Simple type="ota2:Empty"/>
        <ns2:Summary>
            <ns2:Attribute mandatory="true" name="numberOfUnits"
                referenceRepeat="0" type="xsd:integer">
                <ns2:Documentation>
                    <ns2:Description>This is the required number of rooms for a particular day for a particular room type.</ns2:Description>
                    <ns2:Implementer>Example value: rfp1 = 75</ns2:Implementer>
                </ns2:Documentation>
                <ns2:Example context="rfp">75</ns2:Example>
            </ns2:Attribute>
            <ns2:Attribute name="occupancyType" referenceRepeat="0" type="ota2-0500:StringShort">
                <ns2:Documentation>
                    <ns2:Description>Single, Double, Triple, Quad. Not ENUM.</ns2:Description>
                    <ns2:Implementer>Example value: rfp1 = Single</ns2:Implementer>
                </ns2:Documentation>
                <ns2:Example context="rfp">Single</ns2:Example>
            </ns2:Attribute>
            <ns2:Element name="RoomType" repeat="0" type="ph-0200:RoomType">
                <ns2:Documentation>
                    <ns2:Description>This is where a requester specifies the room type they wish to procure (e.g. king, double, double double, accessible). This is NOT an OpenTravel Codelist due to the infinite number of possible room types.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
        </ns2:Summary>
        <ns2:Detail>
            <ns2:Attribute name="comments" referenceRepeat="0" type="ota2-0500:StringTiny">
