    <ns2:CoreObject name="RoomBlock" notExtendable="false">
        <ns2:Documentation>
            <ns2:Description>Specifies the details for a room block.</ns2:Description>
        </ns2:Documentation>
        <ns2:Aliases/>
        <ns2:Simple type="ota2:Empty"/>
        <ns2:Summary>
            <ns2:Attribute name="organizerCheckInDaysBefore"
                referenceRepeat="0" type="xsd:integer">
                <ns2:Documentation>
                    <ns2:Description>Specifies the number of days the organizers will be checking in before the event.</ns2:Description>
                    <ns2:Implementer>Example value: rfp1 = 2</ns2:Implementer>
                </ns2:Documentation>
                <ns2:Example context="rfp">2</ns2:Example>
            </ns2:Attribute>
            <ns2:Attribute name="organizerCheckOutDaysAfter"
                referenceRepeat="0" type="xsd:integer">
                <ns2:Documentation>
                    <ns2:Description>Specifies the number of days the organizers will be staying after the event.</ns2:Description>
                    <ns2:Implementer>Example value: rfp1 = 1</ns2:Implementer>
                </ns2:Documentation>
                <ns2:Example context="rfp">1</ns2:Example>
            </ns2:Attribute>
            <ns2:Element name="BudgetedRoomRate" repeat="0" type="ota2-0500:CurrencyAmount">
                <ns2:Documentation>
                    <ns2:Description>A general indication of the planner's budget for individual room nights and/or delegate rates.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element mandatory="true" name="StayDay" repeat="100" type="StayDay">
                <ns2:Documentation>
                    <ns2:Description>Room requirements for a particular day of the room block.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="ReservationMethod" repeat="0" type="ota2-0300:ReservationMethod_Enum">
                <ns2:Documentation>
                    <ns2:Description>Rooming List, Individual, Housing Agency</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Indicator name="sleepingRoomInd" publishAsElement="false">
                <ns2:Documentation>
                    <ns2:Description>When true, sleeping rooms are required.</ns2:Description>
                </ns2:Documentation>
            </ns2:Indicator>
        </ns2:Summary>
        <ns2:Detail/>
    </ns2:CoreObject>
    
    <ns2:ValueWithAttrs name="SpaceSize" type="xsd:decimal">
        <ns2:Documentation>
            <ns2:Description>The numeric value associated with the measurement.</ns2:Description>
            <ns2:Implementer>Example value: rfp1 = 1200</ns2:Implementer>
            <ns2:Implementer>Example value: rfp1 = 1200</ns2:Implementer>
        </ns2:Documentation>
