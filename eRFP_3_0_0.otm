                <ns2:Documentation>
                    <ns2:Description>The number which indicates the day of the room requirements (e.g. 1, 2, 3, etc).</ns2:Description>
                    <ns2:Implementer>Example value: rfp1 = 1</ns2:Implementer>
                </ns2:Documentation>
                <ns2:Example context="rfp">1</ns2:Example>
            </ns2:Attribute>
            <ns2:Element mandatory="true" name="StayDayRoom"
                repeat="100" type="StayDayRoom">
