            <Documentation>
                <Description>Error or Warning code</Description>
            </Documentation>
        </Attribute>
    </ValueWithAttrs>
    
    <Simple name="Code_Condition" pattern="[A-F0-9]{3}" type="xsd:string">
        <Documentation>
            <Description>An error or warning code.</Description>
            <Deprecated>Replaced with OTM/Common</Deprecated>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
