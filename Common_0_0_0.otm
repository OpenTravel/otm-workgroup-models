    <Simple listTypeInd="true" name="TinyStrings" type="ota1:String_Tiny">
        <Documentation>
            <Description>TODO - How to use it</Description>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
    </Simple>
    
    <Enumeration_Closed name="YesNoInherit_Enum">
        <Documentation>
            <Description/>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Value literal="Yes"/>
        <Value literal="No"/>
