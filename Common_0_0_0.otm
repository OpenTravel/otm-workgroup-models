    <Import fileHints="Common_0_0_0.otm"
        namespace="http://www.opentravel.org/OTM/common/v0" prefix="c"/>
    <Import fileHints="Profile_0_0_0.otm"
        namespace="http://www.opentravel.org/OTM/profile/v0" prefix="prof"/>
    <Context applicationContext="http://opentravel.org/OTM/strawman" context="common"/>
    
    <CoreObject name="Result" notExtendable="false">
        <Documentation>
            <Description/>
            <Deprecated>Moved to OTM/Common</Deprecated>
        </Documentation>
        <Aliases/>
