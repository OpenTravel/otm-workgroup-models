            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Equivalent context="common">OntologyCurrencyType_CurrencyCode</Equivalent>
    </Simple>
    
    <Enumeration_Closed name="Gender_Enum">
        <Documentation>
            <Description/>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Value literal="Male"/>
        <Value literal="Female"/>
        <Value literal="Unknown"/>
    </Enumeration_Closed>
    
    <Simple name="GUID" type="ota1:String_Short">
        <Documentation>
            <Description>A globally unique ID for an object. Objects will typically use the full identifier which includes system context information. This form should only be used when the system that recognizes the GUID is easily determined.</Description>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Example context="common">E5348603fk54390945486gkdfkdioe</Example>
    </Simple>
    
    <Simple listTypeInd="true" name="Languages" type="xsd:language">
