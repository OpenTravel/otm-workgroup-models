            <Documentation>
                <Description>Value to be inherited from closest set ancestor.</Description>
            </Documentation>
        </Value>
    </Enumeration_Closed>
    
    <ValueWithAttrs name="Address_BldgRoom" type="ota1:String">
        <Documentation>
            <Description/>
            <Deprecated>Moved to OTM/Common</Deprecated>
            <Deprecated>Moved to OTM/Common</Deprecated>
        </Documentation>
        <Example context="common">Bldg H</Example>
        <Indicator name="buldingInd" publishAsElement="false">
            <Documentation>
                <Description>When true, the information is a building name.</Description>
            </Documentation>
        </Indicator>
    </ValueWithAttrs>
    
    <ValueWithAttrs name="Address_StreetNumber" type="ota1:String_Short">
        <Documentation>
            <Description/>
            <Implementer>Extension: StreetNmbrType</Implementer>
            <Deprecated>Moved to OTM/Common</Deprecated>
            <Deprecated>Moved to OTM/Common</Deprecated>
        </Documentation>
        <Example context="common">563</Example>
        <Attribute name="streetNmbrSuffix" type="ota1:String_Tiny">
            <Documentation>
                <Description>Usually a letter right after the street number (A in 66-A, B in 123-B etc).</Description>
                <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to8</Implementer>
            </Documentation>
            <Equivalent context="common">StreetNmbrSuffix</Equivalent>
            <Example context="common">B</Example>
        </Attribute>
        <Attribute name="streetDirection" type="ota1:String_Tiny">
            <Documentation>
                <Description>Street direction of an address (e.g., N, E, S, NW, SW).</Description>
                <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to8</Implementer>
            </Documentation>
            <Equivalent context="common">StreetDirection</Equivalent>
            <Example context="common">NNW</Example>
        </Attribute>
        <Attribute name="ruralRouteNmbr" type="c:RuralRouteNumber">
            <Documentation>
                <Description>Numerical equivalent of a rural township as defined within a given area (e.g., 12, 99).</Description>
                <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}NumericStringLength1to5</Implementer>
            </Documentation>
            <Equivalent context="common">RuralRouteNmbr</Equivalent>
            <Example context="common">88</Example>
        </Attribute>
        <Attribute name="pO_Box" type="ota1:String_Short">
            <Documentation>
                <Description>Defines a Post Office Box number.</Description>
            </Documentation>
            <Example context="common">98567499</Example>
        </Attribute>
    </ValueWithAttrs>
    
    <CoreObject name="Address" notExtendable="false">
        <Documentation>
            <Description>Provides address information. Unformatted addresses are captured using the simple facet.</Description>
            <Deprecated>Moved to OTM/Common</Deprecated>
            <Deprecated>Moved to OTM/Common</Deprecated>
        </Documentation>
        <Aliases/>
        <Simple type="ota1:String_Long">
            <Documentation>
                <Description>When the address is unformatted (FormattedInd="false") these lines will contain free form address details. When the address is formatted and street number and street name must be sent independently, the street number will be sent using StreetNmbr, and the street name will be sent in the first AddressLine occurrence.</Description>
                <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to255</Implementer>
            </Documentation>
        </Simple>
        <Summary>
            <Element name="BldgRoom" repeat="2" type="c:Address_BldgRoom">
                <Documentation>
                    <Description>Building name, room, apartment, or suite number.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}BldgRoom</Implementer>
                </Documentation>
                <Equivalent context="common">BldgRoom</Equivalent>
            </Element>
            <Element name="Number" repeat="1" type="c:Address_StreetNumber">
                <Documentation>
                    <Description>May contain the street number and optionally the street name.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StreetNmbr</Implementer>
                </Documentation>
                <Equivalent context="common">StreetNmbr</Equivalent>
            </Element>
            <Element mandatory="true" name="Street" repeat="1" type="ota1:String_Short">
                <Documentation>
                    <Description>May contain the street number when the Street number element is missing.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to255</Implementer>
                </Documentation>
                <Equivalent context="common">AddressLine</Equivalent>
                <Example context="common">Travers Street</Example>
            </Element>
            <Element mandatory="true" name="City" repeat="1" type="ota1:String_Short">
                <Documentation>
                    <Description>City (e.g., Dublin), town, or postal station (i.e., a postal service territory, often used in a military address).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to64</Implementer>
                </Documentation>
                <Equivalent context="common">CityName</Equivalent>
                <Example context="common">Dublin</Example>
            </Element>
            <Element name="County" repeat="1" type="ota1:String_Short">
                <Documentation>
                    <Description>County or Region Name (e.g., Fairfax).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to32</Implementer>
                </Documentation>
                <Equivalent context="common">County</Equivalent>
                <Example context="common">Fairfax</Example>
            </Element>
            <Element name="StateProv" repeat="1" type="c:StateProv">
                <Documentation>
                    <Description>State or Province name (e.g., Texas).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StateProvType</Implementer>
                </Documentation>
                <Equivalent context="common">StateProv</Equivalent>
                <Example context="common">Texas</Example>
            </Element>
            <Element name="Country" repeat="1" type="c:Country">
                <Documentation>
                    <Description>Country name (e.g., Ireland).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}CountryNameType</Implementer>
                </Documentation>
                <Equivalent context="common">CountryName</Equivalent>
                <Example context="common">IE</Example>
            </Element>
            <Element name="PostalCode" repeat="1" type="ota1:String_Short">
                <Documentation>
                    <Description>Post Office Code number.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to16</Implementer>
                </Documentation>
                <Equivalent context="common">PostalCode</Equivalent>
                <Example context="common">32165-9999</Example>
            </Element>
        </Summary>
        <Detail>
            <Attribute name="Language" type="xsd:language">
                <Documentation>
                    <Description>Example: en</Description>
                    <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}language</Implementer>
                </Documentation>
                <Equivalent context="common">Language</Equivalent>
                <Example context="common">en</Example>
            </Attribute>
            <Attribute name="type" type="c:OTA_Code">
                <Documentation>
                    <Description>Defines the type of address (e.g. home, business, other). Refer to OpenTravel Code List Communication Location Type (CLT).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</Implementer>
                </Documentation>
                <Equivalent context="common">Type</Equivalent>
                <Example context="common">CLT</Example>
            </Attribute>
            <Attribute name="use" type="c:OTA_Code">
                <Documentation>
                    <Description>Describes the use of the address (e.g. mailing, delivery, billing, etc.). Refer to OpenTravel Code List Address Use Type (AUT).</Description>
                </Documentation>
                <Example context="common">AUT</Example>
            </Attribute>
            <Element name="Remark" repeat="0" type="ota1:String_Text">
                <Documentation>
                    <Description>A remark associated with this address.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to128</Implementer>
                </Documentation>
                <Equivalent context="common">Remark</Equivalent>
                <Example context="common">This address should be used for all official correspondences.</Example>
            </Element>
            <Element name="Privacy" repeat="0" type="c:Privacy">
                <Documentation>
                    <Description>Allows for control of the sharing of address information between parties.</Description>
                    <Implementer>AttributeGroup: PrivacyGroup</Implementer>
                </Documentation>
            </Element>
            <Indicator name="ValidInd" publishAsElement="false">
                <Documentation>
                    <Description>If true, this is a valid and complete mailing address that has been verified through an address verification service or previously mailed materials have not been returned.</Description>
                </Documentation>
            </Indicator>
        </Detail>
        <Roles>
            <Role value="Home"/>
            <Role value="Business"/>
            <Role value="Destination"/>
            <Role value="Other"/>
        </Roles>
    </CoreObject>
    
    <BusinessObject name="Company" notExtendable="false">
        <Documentation>
            <Description>Detailed information about a company.</Description>
            <Deprecated>Moved to OTM/Common</Deprecated>
        </Documentation>
        <Aliases/>
        <ID>
            <Attribute name="id" type="xsd:ID"/>
            <Element name="Identifier" repeat="0" type="c:Identifier">
                <Documentation>
                    <Description>Identifier provides the ability to create a globally unique identifier. For the identifier to be globally unique it must have a system provided identifier and the system must be identified using a global naming authority. System identification uses the domain naming system (DNS) to assure they are globally unique and should be an URL. The system provided ID will typically be a primary or surrogate key in a database.

The URL, system and company attributes can be omitted only when the system context can be implied by a parent or ancestor element.</Description>
                </Documentation>
            </Element>
        </ID>
        <Summary>
            <Element name="Name" repeat="100000" type="c:Company_Name">
                <Documentation>
                    <Description>Detailed name information for the company.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}CompanyNameType</Implementer>
                </Documentation>
                <Equivalent context="common">CompanyName</Equivalent>
            </Element>
            <Element name="Address" repeat="100000" type="c:Address">
                <Documentation>
                    <Description>A jurisdiction in which a company is authorized to do business. Uses the Address entity to indicate the location of business using any of the components of the address (e.g., city, state, zip code, country).</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}AddressType</Implementer>
                </Documentation>
                <Equivalent context="common">BusinessLocale</Equivalent>
            </Element>
            <Element mandatory="true" name="Telephone" repeat="0" type="c:Telephone">
                <Documentation>
                    <Description>Information about a telephone number, including the actual number and its usage.</Description>
                </Documentation>
            </Element>
            <Element mandatory="true" name="Email" repeat="0" type="c:Email">
                <Documentation>
                    <Description>Electronic email addresses, in IETF specified format.</Description>
                </Documentation>
            </Element>
            <Element name="URL" repeat="100000" type="c:URL">
                <Documentation>
                    <Description>Information on a URL for the company.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}URL_Type</Implementer>
                </Documentation>
                <Equivalent context="common">URL</Equivalent>
            </Element>
            <Element name="Person" repeat="100000" type="prof:Person">
                <Documentation>
                    <Description>Information on a contact person for the company.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}ContactPersonType</Implementer>
                </Documentation>
                <Equivalent context="common">ContactPerson</Equivalent>
            </Element>
            <Element name="RelatedCompany" repeat="100000" type="c:Company_Name">
                <Documentation>
                    <Description>The name of a company affiliated with the employer.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}CompanyNameType</Implementer>
                </Documentation>
                <Equivalent context="common">RelatedEmployer</Equivalent>
            </Element>
        </Summary>
        <Detail>
            <Element name="CurrencyCode" repeat="0" type="c:Currency_Code">
                <Documentation>
                    <Description>The currency used by this company.</Description>
                    <Implementer>AttributeGroup: CurrencyCodeGroup</Implementer>
                </Documentation>
            </Element>
            <Element name="Trip_Purposes" repeat="100000" type="c:Trip_Purposes">
                <Documentation>
                    <Description>Trip purpose codes used by this company.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}TripPurpose</Implementer>
                </Documentation>
                <Equivalent context="common">TripPurpose</Equivalent>
            </Element>
            <Element name="LoyaltyProgram" repeat="100000" type="prof:LoyaltyProgram">
                <Documentation>
                    <Description>Company's loyalty program information.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}LoyaltyProgramType</Implementer>
                </Documentation>
                <Equivalent context="common">LoyaltyProgram</Equivalent>
            </Element>
            <Element name="TravelArranger" repeat="100000" type="prof:TODO_TravelArranger">
                <Documentation>
                    <Description>Information on the organization responsible for arranging travel for the company.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}TravelArrangerType</Implementer>
                </Documentation>
                <Equivalent context="common">TravelArranger</Equivalent>
            </Element>
            <Element name="RequiredPayments" repeat="1" type="prof:RequiredPaymentsEnum">
                <Documentation>
                    <Description>A deposit policy that is associated to this profile.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}RequiredPaymentsType</Implementer>
                </Documentation>
                <Equivalent context="common">DepositPayment</Equivalent>
            </Element>
            <Element name="PaymentForm" repeat="100000" type="prof:PaymentFormEnum-TODO">
                <Documentation>
                    <Description>Method of providing funds for travel by the company.</Description>
                    <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}PaymentForm</Implementer>
                </Documentation>
                <Equivalent context="common">PaymentForm</Equivalent>
            </Element>
        </Detail>
        <Custom context="" label="Employer" notExtendable="false">
            <Attribute name="officeType" type="prof:OfficeLocationEnum">
                <Documentation>
                    <Description>Designates the office category within an organization.</Description>
                    <Implementer>AttributeGroup: OfficeTypeGroup</Implementer>
                </Documentation>
            </Attribute>
            <Element name="EmployeeInfo" repeat="3" type="prof:EmployeeInfo">
