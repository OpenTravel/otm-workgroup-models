            <Description>Specifies an amount, max 3 decimals.</Description>
            <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}decimal</Implementer>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Equivalent context="common">Money</Equivalent>
    </Simple>
    
    <Enumeration_Closed name="NameType">
        <Documentation>
            <Description>OTA Code list: Name Type	NAM	
	1	Former
	2	Nickname
	3	Alternate
	4	Maiden</Description>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Value literal="Alternate"/>
        <Value literal="Former"/>
        <Value literal="Maiden"/>
        <Value literal="Nickname"/>
    </Enumeration_Closed>
    
    <Enumeration_Closed name="OptInStatus_Enum">
        <Documentation>
            <Description/>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Value literal="OptedIn">
            <Documentation>
                <Description>Opted in.</Description>
            </Documentation>
        </Value>
        <Value literal="OptedOut">
            <Documentation>
                <Description>Opted out.</Description>
            </Documentation>
        </Value>
        <Value literal="Unknown">
            <Documentation>
                <Description>Opt in status is unknown.</Description>
            </Documentation>
        </Value>
    </Enumeration_Closed>
    
    <Simple name="OTA_Code"
        pattern="[0-9A-Z]{1,3}(\.[A-Z]{3}(\.X){0,1}){0,1}" type="xsd:string">
        <Documentation>
            <Description>Used for codes in the OpenTravel Code tables. Possible values of this pattern are 1, 101, 101.EQP, or 101.EQP.X.</Description>
            <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
            <Deprecated>Replaced with OTM/Common</Deprecated>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Equivalent context="common">OTA_CodeType</Equivalent>
    </Simple>
    
    <Simple listTypeInd="true" name="OTA_Codes" type="c:OTA_Code">
        <Documentation>
            <Description>List of OpenTravel Codes.</Description>
            <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}OTA_CodeType</Implementer>
            <Deprecated>Replaced with OTM/Common</Deprecated>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Equivalent context="common">ListOfOTA_CodeType</Equivalent>
    </Simple>
    
    <Simple name="RuralRouteNumber" pattern="[0-9]{1,5}" type="xsd:string">
        <Documentation>
            <Description/>
            <Implementer>Example value: Imported5 = 2564</Implementer>
            <Implementer>Example value: Imported4 = 2564</Implementer>
            <Implementer>Example value: Imported2 = 2564</Implementer>
            <Implementer>Example value: Imported67 = 2564</Implementer>
            <Implementer>Example value: Imported123 = 2564</Implementer>
            <Implementer>Example value: Imported34 = 2564</Implementer>
            <Implementer>Example value: ca1 = 25645</Implementer>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Example context="common">2564</Example>
    </Simple>
    
    <Simple maxLength="8" minLength="2" name="StateProv_Code" type="xsd:string">
        <Documentation>
            <Description>The standard code or abbreviation for the state, province, or region.</Description>
            <Implementer>Assigned XSD Type: {http://chameleon.anonymous/ns}StringLength1to8</Implementer>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Equivalent context="common">StateProvCodeType</Equivalent>
    </Simple>
    
    <Simple name="Telephone_AreaCityCode" pattern="[0-9]{1,8}" type="xsd:string">
        <Documentation>
            <Description>Code assigned for telephones in a specific region, city, or area.</Description>
            <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
            <Deprecated>Replaced with OTM/Common</Deprecated>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Equivalent context="common">NumericStringLength1to8</Equivalent>
    </Simple>
    
    <Simple name="Telephone_Extension" pattern="[0-9]{1,5}" type="xsd:string">
        <Documentation>
            <Description>Extension to reach a specific party at the phone number</Description>
            <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
            <Deprecated>Replaced with OTM/Common</Deprecated>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Equivalent context="common">NumericStringLength1to5</Equivalent>
    </Simple>
    
    <Enumeration_Closed name="Text_FormatType">
        <Documentation>
            <Description/>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Value literal="PlainText">
            <Documentation>
                <Description>Textual data that is in ASCII format.</Description>
            </Documentation>
        </Value>
        <Value literal="HTML">
