    <Simple name="Country_Code_ISO3166" pattern="[a-zA-Z]{2}" type="xsd:string">
        <Documentation>
            <Description>Specifies a 2 character country code as defined in ISO3166.</Description>
            <Implementer>Assigned XSD Type: {http://www.w3.org/2001/XMLSchema}string</Implementer>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Equivalent context="common">ISO3166</Equivalent>
        <Example context="common">IR</Example>
    </Simple>
    
    <Simple name="Currency_Code_ISO4217" pattern="[a-zA-Z]{3}" type="xsd:string">
        <Documentation>
            <Description>An ISO 4217 (3) alpha character code that specifies a monetary unit.</Description>
