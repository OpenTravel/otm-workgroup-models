    <Enumeration_Closed name="AccessType_CRUD">
        <Documentation>
            <Description/>
            <Deprecated>Replaced with OTM/Common</Deprecated>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Value literal="Create"/>
        <Value literal="Read"/>
        <Value literal="Update"/>
        <Value literal="Delete"/>
    </Enumeration_Closed>
    
    <Enumeration_Closed name="AccessType_ReadWrite">
        <Documentation>
            <Description>Specifies the type of access that the organization has.</Description>
            <Deprecated>Replaced with OTM/Common</Deprecated>
            <Deprecated>Replaced with OTM/Common</Deprecated>
        </Documentation>
        <Value literal="Read">
            <Documentation>
                <Description>Indicates read only access.</Description>
            </Documentation>
        </Value>
        <Value literal="ReadWrite">
