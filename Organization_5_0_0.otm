    <ns2:BusinessObject name="Facility" notExtendable="false">
        <ns2:Documentation>
            <ns2:Description>Describes a facility (e.g. Hotel Facility,Conference Facility, Golf Facility).  A facility must have a single address and a single geolocation.</ns2:Description>
        </ns2:Documentation>
        <ns2:Aliases>DropOffFacility PickUpFacility</ns2:Aliases>
        <ns2:ID>
            <ns2:Attribute name="objID" referenceRepeat="0" type="xsd:ID">
                <ns2:Documentation>
                    <ns2:Description>A unique identifier within this document for this object.</ns2:Description>
                </ns2:Documentation>
            </ns2:Attribute>
            <ns2:Element name="FacilityRef" reference="true" repeat="0" type="Facility">
                <ns2:Documentation>
                    <ns2:Description>Refers to the obj ID on an object defined elsewhere in the current document.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="FacilityKey" repeat="0" type="FacilityKey">
                <ns2:Documentation>
                    <ns2:Description>Allows for the appropriate type of identifier to be used dependent on the type of facility being described.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
        </ns2:ID>
        <ns2:Summary>
            <ns2:Attribute name="name" referenceRepeat="0" type="ota2-0500:StringShort">
                <ns2:Documentation>
                    <ns2:Description>Name of the facility.</ns2:Description>
                </ns2:Documentation>
            </ns2:Attribute>
            <ns2:Element name="Location" repeat="0" type="ota2-0500:Location_Summary">
                <ns2:Documentation>
                    <ns2:Description>Location information for the facility.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="OperationSchedule" repeat="30" type="ota2-0500:OperationSchedule_Summary">
                <ns2:Documentation>
                    <ns2:Description>Collection of operation schedules.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="RelatedMedia" repeat="0" type="ota2-0501:RelatedMedia">
                <ns2:Documentation>
                    <ns2:Description>Images, descriptions and movies that describe the facility.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Rating" repeat="0" type="ota2-0500:Rating">
                <ns2:Documentation>
                    <ns2:Description>The actual award or rating received by the facility.  May also be used for user generated ratings.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Telephone" repeat="5" type="ota2-0500:Telephone">
                <ns2:Documentation>
                    <ns2:Description>Telephone number for the facility.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Email" repeat="0" type="ota2-0500:Email_Summary">
                <ns2:Documentation>
                    <ns2:Description>Email address for the facility.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Description" repeat="10" type="ota2-0500:Text">
                <ns2:Documentation>
                    <ns2:Description>Facility description.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Property" repeat="0" type="Property">
                <ns2:Documentation>
                    <ns2:Description>An identifierfor the property that this facility is associated with.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="SiteURL" repeat="0" type="ota2-0500:URL">
                <ns2:Documentation>
                    <ns2:Description>The URL for the facility.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="RelativePosition" repeat="0" type="ota2-0500:RelativePosition_Summary">
                <ns2:Documentation>
                    <ns2:Description>The RelativePosition object contains information about the direction, distance and travel to the location.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="FeatureAccess" repeat="100" type="ota2-0501:Feature_Access">
                <ns2:Documentation>
                    <ns2:Description>Identifies an accessibility feature of a property.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="FeatureSecure" repeat="100" type="ota2-0501:Feature_Secure">
                <ns2:Documentation>
                    <ns2:Description>Identifies a security feature of a property.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="FacilityUnitCount" repeat="0" type="FacilityUnitCount">
                <ns2:Documentation>
                    <ns2:Description>Specifies the counts for facility units such as the number of guest rooms, the number of meeting rooms and the number of resaurants in a facility.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Notice" repeat="100" type="ota2-0501:Notice">
                <ns2:Documentation>
                    <ns2:Description>Notices (alerts, promotions etc.) associated with this facility.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="CoLocated" repeat="0" type="ota2-0402:CoLocation_Enum">
                <ns2:Documentation>
                    <ns2:Description>Type of location in which this facility is co-located.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Hours" repeat="7" type="ota2-0500:String">
                <ns2:Documentation>
                    <ns2:Description>Human readable description of hours of operation. Ex. Mon 7 - 5</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
        </ns2:Summary>
        <ns2:Detail/>
    </ns2:BusinessObject>
    <ns2:ContextualFacet facetNamespace="" facetOwner="Facility"
