                    <ns2:Description>The destination system code defines a system to which information is to be provided.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="WeatherInfo" repeat="100" type="ota2-0500:WeatherInfo">
                <ns2:Documentation>
                    <ns2:Description>Provides information regarding the weather.  May be defined by period (e.g. season, month).</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
        </ns2:Summary>
        <ns2:Detail/>
    </ns2:BusinessObject>
    
    <ns2:CoreObject name="ClosedSeason" notExtendable="false">
        <ns2:Documentation>
            <ns2:Description>Specifies date information related to a season that the property is closed.</ns2:Description>
        </ns2:Documentation>
        <ns2:Aliases/>
        <ns2:Simple type="ota2:Empty"/>
        <ns2:Summary>
            <ns2:Element name="DateTimeSpanWindows" repeat="0" type="ota2-0500:DateTimeSpanWindows">
                <ns2:Documentation>
                    <ns2:Description>A date and time period when a hotel facility is closed.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
        </ns2:Summary>
        <ns2:Detail/>
    </ns2:CoreObject>
    
    <ns2:CoreObject name="BlackoutDates" notExtendable="false">
        <ns2:Documentation>
            <ns2:Description>A Blackout Date  during which the hotel cannot guarantee rates and rooms availability due to peak occupancy.</ns2:Description>
        </ns2:Documentation>
        <ns2:Aliases/>
        <ns2:Simple type="ota2:Empty"/>
        <ns2:Summary>
            <ns2:Attribute name="name" referenceRepeat="0" type="ota2-0500:StringShort">
                <ns2:Documentation>
                    <ns2:Description>A descriptive name that identifies the reason for the blackout, e.g : 2012 Olympic Games.</ns2:Description>
                </ns2:Documentation>
            </ns2:Attribute>
            <ns2:Element name="DateTimeSpanWindows" repeat="0" type="ota2-0500:DateTimeSpanWindows">
                <ns2:Documentation>
                    <ns2:Description>The dates this blackout period encompasses.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="TextParagraph" repeat="0" type="ota2-0500:TextParagraph">
                <ns2:Documentation>
                    <ns2:Description>A description of the Blackout Date, which may be multilingual.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
        </ns2:Summary>
        <ns2:Detail/>
    </ns2:CoreObject>
    
    <ns2:CoreObject name="Renovation" notExtendable="false">
        <ns2:Documentation>
            <ns2:Description>Provides renovation information for the property.</ns2:Description>
        </ns2:Documentation>
        <ns2:Aliases/>
        <ns2:Simple type="ota2:Empty"/>
        <ns2:Summary>
            <ns2:Attribute name="percentComplete" referenceRepeat="0" type="ota2-0500:Percent">
                <ns2:Documentation>
                    <ns2:Description>Specifies the level of completion of the renovation.</ns2:Description>
                </ns2:Documentation>
            </ns2:Attribute>
            <ns2:Element name="DateTimeSpanWindows" repeat="0" type="ota2-0500:DateTimeSpanWindows">
                <ns2:Documentation>
                    <ns2:Description>Specifies the anticipated renovation time span.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Area" repeat="0" type="ota2-0500:String">
                <ns2:Documentation>
                    <ns2:Description>Describes the specific area or areas of a hotel being renovated.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="CompletionDate" repeat="0" type="ota2-0500:DateMMYY">
                <ns2:Documentation>
                    <ns2:Description>The month and year the renovation was completed or is due to be completed.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="RelatedMedia" repeat="0" type="ota2-0501:RelatedMedia">
                <ns2:Documentation>
                    <ns2:Description>Multimedia information about the renovation.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Element name="Description" repeat="0" type="ota2-0500:Description">
                <ns2:Documentation>
                    <ns2:Description>Descriptive text that describes the renovation.</ns2:Description>
                </ns2:Documentation>
            </ns2:Element>
            <ns2:Indicator name="immediatePlansInd" publishAsElement="false">
                <ns2:Documentation>
                    <ns2:Description>Indicates that renovation is planned but dates are not finalized.</ns2:Description>
                </ns2:Documentation>
            </ns2:Indicator>
        </ns2:Summary>
        <ns2:Detail/>
    </ns2:CoreObject>
